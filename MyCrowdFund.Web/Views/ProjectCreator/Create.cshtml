@model MyCrowdFund.Options.ProjectCreatorOptions



    <form class="js-create-creator col-md-4" method="post"
          action="@Url.Action("CreateOwner","ProjectCreator")">

        <div class="form-group">
            <label>First Name</label>
            <input type="text" class="form-control js-firstname">
        </div>

        <div class="form-group">
            <label>Last Name</label>
            <input type="text" class="form-control js-lastname">
        </div>

        <div class="form-group">
            <label>Email</label>
            <input type="text" class="form-control js-email">
        </div>

        <div class="form-group">
            <label>Age</label>
            <input type="text" class="form-control js-age">
        </div>

        <div class="form-group">
            <label>Photo</label>
            <input type="text" class="form-control js-photo">
        </div>
        <div class="form-group">
            <label>Username</label>
            <input type="text" class="form-control js-username">
        </div>
        <div class="form-group">
            <label>Password</label>
            <input type="text" class="form-control js-password">
        </div>


        <button type="button" class="btn btn-primary  js-submit-creator ">Submit</button>

    </form>


    <div class="alert alert-danger js-creator-alert" role="alert"
         style="display: none">
    </div>

    <div class="alert alert-success js-creator-success" role="alert"
         style="display: none">
    </div>

